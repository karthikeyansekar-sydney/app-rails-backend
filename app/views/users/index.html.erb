<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <%= link_to user_path(user.id) do %>

          <img class="avatar" src="<%= user.profile_pic %>" alt="">
        <% end%>
        <p><%= user.name %></p>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
